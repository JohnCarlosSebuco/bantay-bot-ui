{
  "expo": {
    "name": "BantayBot Monitor",
    "slug": "bantaybot-monitor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "updates": {
      "enabled": false
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bantaybot.monitor"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1e3c72"
      },
      "package": "com.bantaybot.monitor"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
